{
	"command":"release",
	"usage":"-cuegw",
	"description":"打包、编译、开发测试中的相关工具",
	"option":[
		{
			"op":"-u --uglify [uglify]",
			"desc":"是否压缩(优化),启用内置的优化策略",
			"type":"Boolean",
			"key":"uglify"
		},
		{
			"op":"-w --watch [watch]",
			"desc":"watch功能，检测变化自动打包",
			"type":"Boolean",
			"key":"watch"
		},
		{
			"op":"-p --pack [pack]",
			"desc":"是否打成zip包",
			"type":"String",
			"key":"pack"
		},
		{
			"op":"-s --suffix",
			"desc":"是否添加反缓存后缀",
			"type":"Boolean",
			"key":"suffix"
		},
		{
			"op":"-d --deploy [deploy]",
			"desc":"部署目录，必须为绝对路径，标识服务器访问目录的位置",
			"type":"String",
			"key":"deploy"
		},
		{
			"op":"-b --brower [brower]",
			"desc":"直接打开浏览器,只支持window系统",
			"type":"String",
			"key":"brower"
		}
	]
}