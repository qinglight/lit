{
	"command":"create [directory]",
	"usage":"[directory]",
	"description":"初始化工程目录结构",
	"option":[
		{
			"op":"-t --type <type>",
			"desc":"需要创建的工程类型，默认为单页web工程（mobile）",
			"type":"String",
			"key":"type"
		},
		{
			"op":"-d --description <description>",
			"desc":"工程的描述信息",
			"type":"String",
			"key":"description"
		},
		{
			"op":"-v --version <version>",
			"desc":"创建的工程的版本信息",
			"type":"String",
			"key":"version"
		},
		{
			"op":"-n --name <name>",
			"desc":"工程的名字",
			"type":"String",
			"key":"name"
		},
		{
			"op":"--with-doc",
			"desc":"创建文档目录",
			"type":"Boolean",
			"key":"withDoc"
		}
	]
}

